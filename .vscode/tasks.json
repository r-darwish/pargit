{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "CARGO_ASC=true CARGO_MSG_LIMIT=1 cargo lcheck",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": true
			},
			"label": "cargo check"
		}
	]
}